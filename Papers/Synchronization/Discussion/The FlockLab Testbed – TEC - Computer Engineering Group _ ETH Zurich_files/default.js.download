function DropDown(el) {
	this.dd = el;
	this.placeholder = this.dd.children('span');
	this.opts = this.dd.find('ul.dropdown > li');
	this.val = '';
	this.index = -1;
	this.initEvents();
}

DropDown.prototype = {
	initEvents: function () {
		var obj = this;

		obj.dd.on('click', function (event) {
			$(this).toggleClass('active');
			if ($(this).hasClass('active')) {
				$(this).find('ul.dropdown').attr('aria-hidden', 'false');
				$(this).find('ul.dropdown').attr('aria-expanded', 'true');
			} else {
				$(this).find('ul.dropdown').attr('aria-hidden', 'true');
				$(this).find('ul.dropdown').attr('aria-expanded', 'false');
			}
			return false;
		});

		obj.dd.on('keydown', function (event) {
			if (event.keyCode === 13) {
				$(this).toggleClass('active');
				if ($(this).hasClass('active')) {
					$(this).find('ul.dropdown').attr('aria-hidden', 'false');
					$(this).find('ul.dropdown').attr('aria-expanded', 'true');
				} else {
					$(this).find('ul.dropdown').attr('aria-hidden', 'true');
					$(this).find('ul.dropdown').attr('aria-expanded', 'false');
				}
				return false;
			}
		});

		obj.opts.on('click', function (e) {
			var opt = $(this);
			obj.val = opt.text();
			obj.index = opt.index();
			obj.placeholder.text(obj.val);
			document.location = e.target.href;
		});

		obj.opts.on('keydown', function (e) {
			if (e.keyCode === 13) {
				var opt = $(this);
				obj.val = opt.text();
				obj.index = opt.index();
				obj.placeholder.text(obj.val);
				document.location = e.target.href;
			}
		});


	},
	getValue: function () {
		return this.val;
	},
	getIndex: function () {
		return this.index;
	}
}

