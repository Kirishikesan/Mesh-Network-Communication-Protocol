function format(data) {
	var out = [];
	var sub = [];
	if (data.authors) {
		out.push(data.authors);
	}
	if (data.year) {
		sub.push('(' + data.year + ')');
	}
	if (data.title) {
		sub.push("'" + data.title + "'");
	}
	if (sub) {
		out.push(sub.join(' '));
	}
	if (data.series_title) {
		out.push('<i>' + data.series_title + '<\/i>');
	}
	if (out) {
		return out.join(', ') + ".";
	}
	return '';
}
