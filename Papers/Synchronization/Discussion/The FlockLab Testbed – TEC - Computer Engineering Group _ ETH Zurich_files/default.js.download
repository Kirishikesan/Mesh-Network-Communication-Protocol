eth.sortJobsTable = (function ($, Anzeixer) {
	'use strict';

	$(window).load(function () {
		sortJobs();
	});


	var sortJobs = (function () {

		if (Anzeixer.getView() === 'xs portrait' || Anzeixer.getView() === 's portrait' || Anzeixer.getView() === 's landscape') {
			if ($('.jobs.tablesorter').length && $('#job-sorter-form').length) {

				$('#sorter-select').on('change', function (e) {

					var sorting;
					var so = 0;
					var sortOrder = $('input[name="sort-order"]:checked').val(); // default = asc
					if (sortOrder === 'desc') {
						so = 1;
					}
					var sel = e.target.value;

					switch (sel) {
						case 'descr':
							sorting = [[0, so]];
							break;
						case 'workload':
							sorting = [[1, so]];
							break;
						case 'org':
							sorting = [[2, so]];
							break;
						case 'date':
							sorting = [[3, so]];
							break;
						default:
							console.log('error');
							break;
					}

					console.log(so);
					console.log(sel);
					console.log(sorting);
					console.log(sortOrder);

					$('table.jobs.tablesorter').trigger('sorton', [sorting]);
					return false;

				});


			}
		}
	});

}(jQuery, Anzeixer));

if ($ && $.tablesorter) {
	$.tablesorter.addParser({
		id: "jobDate",
		is: function (s) {
			return /^[0-9\.]{10}$/.test(s);
		},
		format: function (s) {
			var parts = s.split('.');
			if (parts.length === 3) {
				return parts[2] + parts[1] + parts[0];
			}
			return s;
		},
		type: "numeric"
	});

	$(document).ready(function () {
		$('.jobs.tablesorter').tablesorter({
			headers: {
